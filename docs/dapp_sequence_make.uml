@startuml

actor User
participant DApp
database TBA

User -> DApp : Setup order
activate DApp
User <- DApp : Pair, type, volume, price?
User -> DApp
DApp -> TBA : Check stake
alt enough stake
  DApp <- TBA : Stake enough
  User <- DApp : Sign
  User -> DApp : Signature
  DApp -> TBA : Order
else insufficient stake
  DApp <- TBA : Insufficient stake
  User <- DApp : Error
  deactivate DApp
end

@enduml
